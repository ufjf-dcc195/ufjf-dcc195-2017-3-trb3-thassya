<%- contentFor('body') %>
<div class="panel panel-success">
  <div class="panel-heading"><h3>Registrar Interesse em <%=produto.nome%></h3></div>
  <div class="panel-body">
    <table>
      <tr>
        <td>
          <form class="form-inline" method="POST" action="/adicionarInteresse.html">
            <div class="form-group">
            <select name="interesse"  class="form-control">
              <option>Sem interesse</option>
              <option>Baixo interesse</option>
              <option>Tenho interesse</option>
              <option>Muito interessado</option>
            </select>
          </div>
          <button type="submit" class="btn btn-default">Registrar</button>
          <button type="reset" class="btn btn-default">Limpar Campos</button>
          </form>
        </td>
      </tr>
    </table>
</div>
</div>
